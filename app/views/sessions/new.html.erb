<p id=”alert”><%= alert %></p>

<h1>Login</h1>

<%= form_with method: 'POST', url: '/authenticate' do | f | %>
    <div>
        <%= f.label :username %>
        <%= f.text_field :username %>
    </div>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
    <%= f.submit("Login") %>
<% end %>